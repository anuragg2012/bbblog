{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bb": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/bb",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/web/assets/mobirise-icons2"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/bootstrap/css/bootstrap.min.css",
              "src/assets/theme/css/style.css",
              "src/assets/socicon/css/styles.css",
              "src/assets/dropdown/css/style.css",
              "src/assets/parallax/jarallax.css",
              "src/assets/mobirise/css/additional.css"
            ],
            "scripts": [
              "src/assets/parallax/jarallax.js",
              "src/assets/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/dropdown/js/navbar-dropdown.js",
              "src/assets/scrollgallery/scroll-gallery.js",
              "src/assets/masonry/masonry.pkgd.min.js",
              "src/assets/imagesloaded/imagesloaded.pkgd.min.js",
              "src/assets/mbr-switch-arrow/mbr-switch-arrow.js",
              "src/assets/smoothscroll/smooth-scroll.js",
              "src/assets/theme/js/script.js",
              "src/assets/ytplayer/index.js",
              "src/assets/formoid/formoid.min.js",
              "src/app/main.js"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "25kb",
                  "maximumError": "25kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "bb:build:production"
            },
            "development": {
              "buildTarget": "bb:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "bb:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "4c34779b-e568-497e-a2ab-e37f8a2a5133"
  }
}
